# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your-firebase-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-firebase-auth-domain
REACT_APP_FIREBASE_PROJECT_ID=your-firebase-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-firebase-storage-bucket
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-firebase-messaging-sender-id
REACT_APP_FIREBASE_APP_ID=your-firebase-app-id

# Backend API
REACT_APP_BACKEND_URL=http://localhost:5000

# INApas SSO Configuration
REACT_APP_INAPAS_AUTH_URL=https://dev-inapass-api.govtechindonesia.id
REACT_APP_INAPAS_CLIENT_ID=your-inapas-client-id
# The redirect URI will be automatically set based on environment:
# - Development: Uses this value or defaults to http://localhost:3000/inapas/callback
# - Production: Always uses https://admin.inatrading.co.id/inapas/callback
REACT_APP_INAPAS_REDIRECT_URI=http://localhost:3000/inapas/callback
REACT_APP_INAPAS_KID=your-kid-value-from-jwks

# INApas Private Keys (should be securely stored and not included in client-side code in production)
# These are placeholders for local development only
REACT_APP_INAPAS_SIGNING_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_SIGNING_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----"
REACT_APP_INAPAS_ENCRYPTION_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_ENCRYPTION_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----" 